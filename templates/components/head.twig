	<meta charset="{{site.charset}}" />

	<title>   
		{% if wp_title %}
			{{ wp_title }} &middot; {{ site.name }}
		{% else %}
			{{ site.name }}
		{% endif %}
	</title>

	<meta name="description" content="{{site.description}}">

	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	
	<!-- Google Fonts -->
	<link href="https://fonts.googleapis.com/css?family=Chivo:300,300i,400,400i,700,700i,900,900i&amp;subset=latin-ext" rel="stylesheet"> 

	<!-- Style sheets -->
	<link rel="stylesheet" href="{{site.theme.link}}/style.css" type="text/css" media="screen" />

	<link rel="pingback" href="{{site.pingback_url}}" />
	
	{{function('wp_head')}}

